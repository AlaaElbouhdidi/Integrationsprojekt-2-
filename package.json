{
    "name": "mate-team",
    "version": "0.2.0",
    "description": "Firebase powered API for Mate Team",
    "homepage": "https://mate-team.de",
    "repository": {
        "type": "git",
        "url": "https://git.thm.de/cbkr81/integrationsprojekt-2"
    },
    "license": "MIT",
    "private": true,
    "engines": {
        "node": "14"
    },
    "main": "dist/apps/api/main.js",
    "scripts": {
        "ng": "nx",
        "test": "tools/scripts/test.sh",
        "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
        "preprod": "npm run all:build:prod",
        "serve:dev": "concurrently \"npm run client:start:dev\" \"npm run api:start:dev\"",
        "serve:prod": "concurrently \"npm run client:start:prod\" \"npm run api:start:prod\"",
        "preserve": "npm run all:build:dev",
        "dev": "concurrently \"npm run serve:dev\" \"npm run serve\"",
        "prod": "concurrently \"npm run serve:prod\" \"npm run serve\"",
        "serve": "firebase emulators:start --import=.firebase --export-on-exit",
        "serve:api": "firebase emulators:start --only functions --import=.firebase --export-on-exit",
        "serve:api:dev": "concurrently \"npm run api:build:dev\" \"npm run serve:api\"",
        "serve:api:prod": "concurrently \"npm run api:build:prod\" \"npm run serve:api\"",
        "serve:client": "firebase emulators:start --only hosting --import=.firebase --export-on-exit",
        "client:test": "nx test",
        "client:build:dev": "nx build --configuration=development --skip-nx-cache",
        "client:build:prod": "nx build --skip-nx-cache",
        "client:start:dev": "nx serve --configuration=development",
        "client:start:prod": "nx serve",
        "api:test": "nx test api",
        "api:build:dev": "nx build api --configuration=development --skip-nx-cache",
        "api:build:prod": "nx build api --skip-nx-cache",
        "api:start:dev": "nx serve api --configuration=development",
        "api:start:prod": "nx serve api",
        "all:build:dev": "nx run-many --target=build --all --parallel --configuration=development --skip-nx-cache",
        "all:build:prod": "nx run-many --target=build --all --parallel --skip-nx-cache",
        "all:lint": "nx run-many --target=lint --all --parallel --skip-nx-cache",
        "all:doc": "nx run-many --target=compodoc --all --parallel --skip-nx-cache",
        "all:build": "nx run-many --target=build --all --parallel --skip-nx-cache",
        "all:test": "nx run-many --target=test --all --parallel --skip-nx-cache",
        "all:e2e": "nx run-many --target=e2e --all --parallel --skip-nx-cache",
        "affected:lint": "nx affected --target=lint --parallel --max-parallel=3",
        "affected:test": "nx affected --target=test --parallel  --max-parallel=3",
        "affected:build": "nx affected --target=build --parallel  --max-parallel=3 --skip-nx-cache",
        "affected:doc": "nx affected --target=compodoc --parallel  --max-parallel=3",
        "affected:e2e": "nx affected --target=e2e",
        "format": "nx format:write",
        "format:check": "nx format:check --files apps/**/*.ts libs/**/*.ts",
        "deps": "nx dep-graph",
        "help": "nx help"
    },
    "dependencies": {
        "@angular/animations": "13.0.3",
        "@angular/common": "13.0.3",
        "@angular/compiler": "13.0.3",
        "@angular/core": "13.0.3",
        "@angular/fire": "^7.1.1",
        "@angular/forms": "13.0.3",
        "@angular/localize": "^12.2.14",
        "@angular/platform-browser": "13.0.3",
        "@angular/platform-browser-dynamic": "13.0.3",
        "@angular/platform-server": "13.0.3",
        "@angular/pwa": "^13.0.4",
        "@angular/router": "13.0.3",
        "@angular/service-worker": "^12.2.14",
        "@compodoc/compodoc": "^1.1.16",
        "@fortawesome/angular-fontawesome": "^0.9.0",
        "@fortawesome/fontawesome-svg-core": "^1.2.36",
        "@fortawesome/free-solid-svg-icons": "^5.15.4",
        "@nestjs/common": "^8.2.0",
        "@nestjs/config": "^1.1.5",
        "@nestjs/core": "^8.2.0",
        "@nestjs/ng-universal": "^4.2.0",
        "@nestjs/passport": "^8.0.1",
        "@nestjs/platform-express": "^8.2.0",
        "@nestjs/platform-socket.io": "^8.2.3",
        "@nestjs/serve-static": "^2.2.2",
        "@nestjs/swagger": "^5.1.4",
        "@nestjs/websockets": "^8.2.3",
        "@ng-bootstrap/ng-bootstrap": "^10.0.0",
        "@nguniversal/builders": "^13.0.1",
        "@nguniversal/express-engine": "^11.0.1",
        "@nrwl/angular": "13.2.4",
        "@types/socket.io-client": "^3.0.0",
        "bootstrap": "^5.1.3",
        "bootswatch": "^5.1.3",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.13.2",
        "concurrently": "^6.4.0",
        "express": "^4.15.2",
        "firebase": "^9.6.0",
        "firebase-admin": "^10.0.0",
        "firebase-functions": "^3.16.0",
