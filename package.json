{
    "name": "mate-team",
    "version": "0.2.0",
    "description": "Firebase powered API for Mate Team",
    "homepage": "https://mate-team.de",
    "repository": {
        "type": "git",
        "url": "https://git.thm.de/cbkr81/integrationsprojekt-2"
    },
    "license": "MIT",
    "private": true,
    "scripts": {
        "ng": "nx",
        "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
        "start": "nx serve",
        "build": "nx build mate-team --configuration=development --skip-nx-cache",
        "serve": "firebase emulators:start --import=.firebase --export-on-exit",
        "prebuild": "nx run-many --target=compodoc --parallel=true --all=true",
        "preserve": "npm run build",
        "api:serve": "nx run api:serve",
        "test": "nx test",
        "lint": "nx workspace-lint && ng lint",
        "e2e": "ng e2e",
        "format": "nx format:write",
        "format:check": "nx format:check --files apps/**/*.ts, libs/**/*.ts",
        "update": "ng update @nrwl/workspace",
        "dep-graph": "nx dep-graph",
        "help": "nx help"
    },
