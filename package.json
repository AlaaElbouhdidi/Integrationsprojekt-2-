        "eslint-plugin-cypress": "^2.12.1",
        "firebase-functions-test": "^0.2.0",
        "jest": "27.2.3",
        "jest-preset-angular": "11.0.0",
        "ng-packagr": "^12.2.6",
        "postcss": "^8.4.4",
        "postcss-import": "^14.0.2",
        "postcss-preset-env": "^6.7.0",
        "postcss-url": "^10.1.1",
        "prettier": "2.5.1",
        "ts-jest": "27.0.5",
        "ts-node": "~9.1.1",
        "typescript": "4.4.4"
    }
}
